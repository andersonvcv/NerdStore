@model IEnumerable<NerdStore.Core.Data.EventSourcing.StoredEvent>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    @{
        <table class="table table-sm table-bordered table-hover col-md-12">
            <thead class="table-dark">
            <tr>
                <td>Id</td>
                <td>Name</td>
                <td>Data</td>
                <td>JSON</td>
            </tr>
            </thead>
            <tbody>
            @foreach (var esEvent in Model)
            {
                <tr>
                    <td>@esEvent.Id</td>
                    <td>@esEvent.Type</td>
                    <td>@esEvent.EntryDate</td>
                    <td><code>@esEvent.Data</code></td>
                </tr>
            }
            </tbody>
        </table>


    }
</div>